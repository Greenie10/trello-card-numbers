{
  "manifest_version": 2,

  "name": "Trello Card Numbers",
  "description": "Adds card numbers to a variety of places when using Trello.",
  "version": "1.1.1",

  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png" },

    "permissions": [
      "https://trello.com/*"
    ],

    "content_scripts": [
      {
        "matches": ["https://trello.com/*"],
        "js": ["trello-card-numbers.js"]
      }
    ]
}
